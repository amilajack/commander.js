{
  "name": "commander",
  "version": "2.19.0",
  "description": "the complete solution for node.js command-line programs",
  "keywords": [
    "commander",
    "command",
    "option",
    "parser"
  ],
  "author": "TJ Holowaychuk <tj@vision-media.ca>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tj/commander.js.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "docs": "typedoc --out docs src --ignoreCompilerErrors",
    "lint": "eslint .",
    "spec": "node -r @babel/register test/run.js",
    "test": "yarn tsc && node test/run.js && yarn build && yarn doc && yarn lint",
    "ts": "tsc"
  },
  "main": "./lib",
  "files": [
    "index.js",
    "typings/index.d.ts"
  ],
  "dependencies": {
    "omelette": "^0.4.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@types/node": "^11.9.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.0",
    "eslint-config-bliss": "^3.3.0",
    "jest-cli": "^24.1.0",
    "should": "^13.2.3",
    "sinon": "^7.2.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3"
  },
  "eslintConfig": {
    "extends": "bliss"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10
          }
        }
      ],
      [
        "@babel/preset-typescript",
        {
          "allExtensions": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
